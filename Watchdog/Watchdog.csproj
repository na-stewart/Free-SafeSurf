<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <AssemblyName>svchost</AssemblyName>
    <ApplicationManifest>app.manifest</ApplicationManifest>
    <Title>SSWatchdog</Title>
    <Company></Company>
    <Product>SSWatchdog</Product>
    <Authors></Authors>
    <StartupObject>Watchdog.Program</StartupObject>
    <ApplicationIcon>safe-surf.ico</ApplicationIcon>
    <Version>0.2</Version>
    <AssemblyVersion>0.2</AssemblyVersion>
    <FileVersion>0.2</FileVersion>
    <Copyright>Copyright (c) 2024 Nicholas Aidan Stewart</Copyright>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="safe-surf.ico" />
  </ItemGroup>

</Project>